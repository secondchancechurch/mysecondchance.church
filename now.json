{
  "name": "scc-website",
  "version": 2,
  "public": false,
  "alias": [
    "www.mysecondchancechurch.com",
    "mysecondchancechurch.com"
  ],
  "routes": [
    {
      "src": "/give/now",
      "status": 301,
      "headers": { "Location": "https://pushpay.com/pay/secondchancechurch" }
    },{
      "src": "/easter",
      "status": 301,
      "headers": { "Location": "/" }
    },{
      "src": "/sermons",
      "dest": "https://youtube.com/c/secondchancechurch"
    },{
      "src": "/(?<slug>[^/]*)",
      "dest": "/?slug=$slug"
    }
  ],
  "builds": [
    { "src": "./lib/schemaQuery.js", "use": "@now/node"},
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "build": {
    "env": {
      "GRAPHQL_ENDPOINT": "@sc-graphql-endpoint",
      "FONT_AWESOME_TOKEN": "@fa-token"
    }
  }
}